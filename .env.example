# PostgreSQL Database Connection (Railway sets this automatically)
DATABASE_URL=postgresql://user:password@localhost:5432/delicious

# Payload CMS Secret (generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
PAYLOAD_SECRET=your-secure-random-secret-key-min-32-characters

# Node Environment
NODE_ENV=production

# Optional: Admin URL (set this to your Railway domain)
PAYLOAD_ADMIN_URL=https://your-domain.railway.app/admin
